package emailApp;

public class emailApp 
{

	public static void main(String[] args) 
	{
		email em1 = new email("John", "Smith");
		em1.setAlternateEmail("mb@email.com");
		//System.out.printf("\nAlternate email: %s ",em1.getAlternateEmail());
		em1.setMailBoxCapacity(1000);
		System.out.printf("\nMailbox capacity %smb",em1.getMailboxCapacity());
		em1.changePassword("dsfahjkl3!@E##DS");
		System.out.printf("\nNew password is: %s", em1.getPassword());
		System.out.printf("\n%s",em1.showInfo());


	}

}
